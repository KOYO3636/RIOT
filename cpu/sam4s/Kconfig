config CPU_FAM_SAM4S
    bool
    select CPU_CORE_CORTEX_M4

## CPU Models
config CPU_MODEL_SAM4SD32C
    bool
    select CPU_FAM_SAM4S

## Common CPU symbols
config CPU_FAM
    default "sam4s" if CPU_FAM_SAM4S

config CPU_MODEL
    default "sam4sd32c" if CPU_MODEL_SAM4SD32C

config CPU
    default "sam4s" if CPU_FAM_SAM4s

source "$(RIOTCPU)/cortexm_common/Kconfig"
