config CPU_FAM_NRF9160
    bool

## CPU Models
config CPU_MODEL_NRF9160
    bool
    select CPU_CORE_CORTEX_M33
    select CPU_FAM_NRF9160

## CPU common symbols
config CPU_FAM
    default "nrf9160" if CPU_FAM_NRF9160

config CPU_MODEL
    default "nrf9160" if CPU_MODEL_NRF9160

config CPU
    default "nrf9160" if CPU_FAM_NRF9160

source "$(RIOTCPU)/nrf5x_common/Kconfig"
