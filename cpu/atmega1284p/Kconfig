## CPU Models
config CPU_MODEL_ATMEGA1284P
    bool
    select CPU_FAM_ATMEGA128

## Common CPU symbols
config CPU_MODEL
    default "atmega1284p" if CPU_MODEL_ATMEGA1284P

config CPU
    default "atmega1284p" if CPU_MODEL_ATMEGA1284P

source "$(RIOTCPU)/atmega_common/Kconfig"
