config CPU_ARCH_ARMV4T
    bool

config CPU_CORE_ARM7TDMI_S
    bool
    select CPU_ARCH_ARMV4T

## Common CPU symbols
config CPU_ARCH
    default "armv4t" if CPU_ARCH_ARMV4T

config CPU_CORE
    default "arm7tdmi_s" if CPU_CORE_ARM7TDMI_S
